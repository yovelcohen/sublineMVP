            <ns0:p xml:id="subtitle1012" begin="16199102502t" end="16217035000t" region="region38" style="style0">
                בעצם,
            </ns0:p>
            <ns0:p xml:id="subtitle1013" begin="16199102502t" end="16217035000t" region="region34" style="style0">אני
                הייתי בטלפון איתם,
            </ns0:p>
            <ns0:p xml:id="subtitle1014" begin="16217452500t" end="16245405008t" region="region4" style="style0">ולואיס
                עזר לי--
            </ns0:p>
            <ns0:p xml:id="subtitle1015" begin="16217452500t" end="16245405008t" region="region63" style="style0">- LOOK
                AT
                ME.
            </ns0:p>
            <ns0:p xml:id="subtitle1016" begin="16245822508t" end="16266667500t" region="region30" style="style0">- והוא
                עשה את זה
            </ns0:p>
            <ns0:p xml:id="subtitle1017" begin="16245822508t" end="16266667500t" region="region34" style="style0">הדבר
                הזה, וגרגורי--
            </ns0:p>
            <ns0:p xml:id="subtitle1018" begin="16267085000t" end="16297532500t" region="region63" style="style0">-
                הסתכל אליי.
            </ns0:p>
            <ns0:p xml:id="subtitle1019" begin="16345087508t" end="16376359999t" region="region26" style="style0">- אתה
                גבוה.
            </ns0:p>
            <ns0:p xml:id="subtitle1020" begin="16389292502t" end="16407642500t" region="region158" style="style0">צא.
            </ns0:p>
            <ns0:p xml:id="subtitle1021" begin="16408060001t" end="16423080006t" region="region33" style="style0">- (
                sighs
                )
            </ns0:p>
            <ns0:p xml:id="subtitle1022" begin="16423497506t" end="16443100006t" region="region10" style="style0">
                הארווי, אתה לא מבין.
            </ns0:p>
            <ns0:p xml:id="subtitle1023" begin="16423497506t" end="16443100006t" region="region131" style="style0">
                לואיס--
            </ns0:p>
            <ns0:p xml:id="subtitle1024" begin="16443517506t" end="16462285005t" region="region134" style="style0">-
                צא.
            </ns0:p>
            <ns0:p xml:id="subtitle1025" begin="16584910007t" end="16600337502t" region="region134" style="style0">
                דונה?
            </ns0:p>
            <ns0:p xml:id="subtitle1026" begin="16600755003t" end="16632455005t" region="region36" style="style0">את
                יכולה להביא
            </ns0:p>
            <ns0:p xml:id="subtitle1027" begin="16600755003t" end="16632455005t" region="region15" style="style0">את
                וויאט בחזרה לקו הטלפון?
            </ns0:p>
            <ns0:p xml:id="subtitle1028" begin="16793867506t" end="16813470006t" region="region34" style="style0">- מה
                אתה עושה פה?
            </ns0:p>
            <ns0:p xml:id="subtitle1029" begin="16813887506t" end="16836819999t" region="region15" style="style0">- זה
                השירותים לנשים.
            </ns0:p>
            <ns0:p xml:id="subtitle1030" begin="16837237500t" end="16872277505t" region="region36" style="style0">- (
                sighs
                )
            </ns0:p>
            <ns0:p xml:id="subtitle1031" begin="16837237500t" end="16872277505t" region="region7" style="style0">אני
                מצטער.
            </ns0:p>
            <ns0:p xml:id="subtitle1032" begin="16872695005t" end="16902725005t" region="region20" style="style0">תראי,
                את יכולה לעשות לי טוב?
            </ns0:p>
            <ns0:p xml:id="subtitle1033" begin="16872695005t" end="16902725005t" region="region158" style="style0">
                אפשר
            </ns0:p>
            <ns0:p xml:id="subtitle1034" begin="16903142506t" end="16927745000t" region="region4" style="style0">אנחנו
                רק נשמור על זה
            </ns0:p>
            <ns0:p xml:id="subtitle1035" begin="16903142506t" end="16927745000t" region="region21" style="style0">הסוד
                שלנו?
            </ns0:p>
            <ns0:p xml:id="subtitle1036" begin="16928162500t" end="16955280007t" region="region25" style="style0">- למה
                העיניים שלך אדומות כל כך?
            </ns0:p>
            <ns0:p xml:id="subtitle1037" begin="16955697508t" end="16983222506t" region="region21" style="style0">(
                שימוש בשירותים )
            </ns0:p>
            <ns0:p xml:id="subtitle1038" begin="16983640006t" end="17014505006t" region="region21" style="style0">- אני
                צריך אוויר.
            </ns0:p>
            <ns0:p xml:id="subtitle1039" begin="17014922507t" end="17037855000t" region="region66" style="style0">- אתה
                לקחת את מייק רוס לטיול שדה היום?
            </ns0:p>
            <ns0:p xml:id="subtitle1040" begin="17014922507t" end="17037855000t" region="region37" style="style0">-
                כן.
            </ns0:p>
            <ns0:p xml:id="subtitle1041" begin="17038272500t" end="17057457500t" region="region54" style="style0">- מלבד
                להציב אותו
            </ns0:p>
            <ns0:p xml:id="subtitle1042" begin="17038272500t" end="17057457500t" region="region25" style="style0">
                במגבת,
            </ns0:p>
            <ns0:p xml:id="subtitle1043" begin="17057875000t" end="17072895005t" region="region35" style="style0">מה
                קרה?
            </ns0:p>
            <ns0:p xml:id="subtitle1044" begin="17057875000t" end="17072895005t" region="region26" style="style0">- טוב,
                אני ניצחתי אותו
            </ns0:p>
            <ns0:p xml:id="subtitle1045" begin="17073312506t" end="17091662503t" region="region4" style="style0">במערכות
                ישרות,
            </ns0:p>
            <ns0:p xml:id="subtitle1046" begin="17073312506t" end="17091662503t" region="region26" style="style0">אם זה
                מה שאתה מקבל--
            </ns0:p>
            <ns0:p xml:id="subtitle1047" begin="17092080003t" end="17109595001t" region="region18" style="style0">- אל
                תתנהג כמו טיפש, לואיס.
            </ns0:p>
            <ns0:p xml:id="subtitle1048" begin="17092080003t" end="17109595001t" region="region46" style="style0">ספר לי
                מה עשית לו.
            </ns0:p>
            <ns0:p xml:id="subtitle1049" begin="17110012502t" end="17125867508t" region="region20" style="style0">- מה
                אתה, קנאי?
            </ns0:p>
            <ns0:p xml:id="subtitle1050" begin="17110012502t" end="17125867508t" region="region15" style="style0">
                הארווי, בוא, הילד
            </ns0:p>
            <ns0:p xml:id="subtitle1051" begin="17126285008t" end="17144635006t" region="region172" style="style0">רעב
                למנטור,
            </ns0:p>
            <ns0:p xml:id="subtitle1052" begin="17126285008t" end="17144635006t" region="region46" style="style0">מישהו
                שיתן לו
            </ns0:p>
            <ns0:p xml:id="subtitle1053" begin="17145052507t" end="17159645001t" region="region51" style="style0">כמה
                עצה.
            </ns0:p>
            <ns0:p xml:id="subtitle1054" begin="17145052507t" end="17159645001t" region="region42" style="style0">אז
                אולי אם
            </ns0:p>
            <ns0:p xml:id="subtitle1055" begin="17160062502t" end="17176752508t" region="region3" style="style0">אתה
                פשוט תיקח את הזמן--
            </ns0:p>
            <ns0:p xml:id="subtitle1056" begin="17160062502t" end="17176752508t" region="region0" style="style0">- אני
                אומר לך עכשיו,
            </ns0:p>
            <ns0:p xml:id="subtitle1057" begin="17177159999t" end="17197180000t" region="region39" style="style0">אתה
                מנסה להתערב בו--
            </ns0:p>
            <ns0:p xml:id="subtitle1058" begin="17177159999t" end="17197180000t" region="region155" style="style0">-
            </ns0:p>
            <ns0:p xml:id="subtitle1059" begin="17197597500t" end="17210530002t" region="region38" style="style0">-
                HARVEY,
            </ns0:p>
            <ns0:p xml:id="subtitle1060" begin="17197597500t" end="17210530002t" region="region13" style="style0">מאיפה
                זה בא?
            </ns0:p>
            <ns0:p xml:id="subtitle1061" begin="17210947502t" end="17228045000t" region="region168" style="style0">-
            </ns0:p>
            <ns0:p xml:id="subtitle1062" begin="17210947502t" end="17228045000t" region="region65" style="style0">יש לי
                את היועץ החיצוני
            </ns0:p>
            <ns0:p xml:id="subtitle1063" begin="17228462500t" end="17259745001t" region="region41" style="style0">
                למהירות בקו.
            </ns0:p>
            <ns0:p xml:id="subtitle1064" begin="17228462500t" end="17259745001t" region="region37" style="style0">- העבר
                אותם.
            </ns0:p>
            <ns0:p xml:id="subtitle1065" begin="17321475003t" end="17342330003t" region="region4" style="style0">- מה
                קרה אחרי
            </ns0:p>
            <ns0:p xml:id="subtitle1066" begin="17321475003t" end="17342330003t" region="region46" style="style0">שעינית
                עם הבחור הזה?
            </ns0:p>
            <ns0:p xml:id="subtitle1067" begin="17342747505t" end="17362350003t" region="region9" style="style0">- פשוט
                התחלתי להיות פרנואידי.
            </ns0:p>
            <ns0:p xml:id="subtitle1068" begin="17362767505t" end="17382787505t" region="region29" style="style0">הרגשתי
                רע על השקר לו,
            </ns0:p>
            <ns0:p xml:id="subtitle1069" begin="17383205005t" end="17411982503t" region="region4" style="style0">אמרתי
                לו שלואיס
            </ns0:p>
            <ns0:p xml:id="subtitle1070" begin="17383205005t" end="17411982503t" region="region2" style="style0">הציע
                לי
            </ns0:p>
            <ns0:p xml:id="subtitle1071" begin="17412400003t" end="17425750007t" region="region23" style="style0">ושהוא
                לא יכול
            </ns0:p>
            <ns0:p xml:id="subtitle1072" begin="17412400003t" end="17425750007t" region="region7" style="style0">לסמוך
                עליו.
            </ns0:p>
            <ns0:p xml:id="subtitle1073" begin="17426167508t" end="17441177502t" region="region32" style="style0">- אז
                מה
            </ns0:p>
            <ns0:p xml:id="subtitle1074" begin="17441595003t" end="17463702506t" region="region23" style="style0">הוא
                אמר?
            </ns0:p>
            <ns0:p xml:id="subtitle1075" begin="17441595003t" end="17463702506t" region="region25" style="style0">- אמר
                שלואיס
            </ns0:p>
            <ns0:p xml:id="subtitle1076" begin="17464120006t" end="17479130001t" region="region38" style="style0">נתן לו
                את הצמרמורת בכל מקרה.
            </ns0:p>
            <ns0:p xml:id="subtitle1077" begin="17464120006t" end="17479130001t" region="region13" style="style0">אתה
                יודע,
            </ns0:p>
            <ns0:p xml:id="subtitle1078" begin="17479547501t" end="17495820007t" region="region16" style="style0">-
                OKAY,
            </ns0:p>
            <ns0:p xml:id="subtitle1079" begin="17479547501t" end="17495820007t" region="region11" style="style0">-
                בסדר,
            </ns0:p>
            <ns0:p xml:id="subtitle1080" begin="17506665008t" end="17525850007t" region="region6" style="style0">אתה
                צריך לספר להרווי.
            </ns0:p>
            <ns0:p xml:id="subtitle1081" begin="17506665008t" end="17525850007t" region="region0" style="style0">-
                ניסיתי,
            </ns0:p>
            <ns0:p xml:id="subtitle1082" begin="17526267508t" end="17543782506t" region="region30" style="style0">הוא לא
                היה מקשיב.
            </ns0:p>
            <ns0:p xml:id="subtitle1083" begin="17526267508t" end="17543782506t" region="region9" style="style0">- אז לך
                הביתה
            </ns0:p>
            <ns0:p xml:id="subtitle1084" begin="17544200006t" end="17565472507t" region="region61" style="style0">וספר
                לו מחר.
            </ns0:p>
            <ns0:p xml:id="subtitle1085" begin="17544200006t" end="17565472507t" region="region26" style="style0">-
                אלוהי, אתה היית צריך
            </ns0:p>
            <ns0:p xml:id="subtitle1086" begin="17565890007t" end="17597580000t" region="region12" style="style0">לראות
                את הפנים שלו.
            </ns0:p>
            <ns0:p xml:id="subtitle1087" begin="17565890007t" end="17597580000t" region="region42" style="style0">היה
                לנו הסכם.
            </ns0:p>
            <ns0:p xml:id="subtitle1088" begin="17630950002t" end="17649300001t" region="region20" style="style0">- איזה
                הסכם?
            </ns0:p>
            <ns0:p xml:id="subtitle1089" begin="17630950002t" end="17649300001t" region="region44" style="style0">- תודה
                ששמעת אותי.
            </ns0:p>
            <ns0:p xml:id="subtitle1090" begin="17649717501t" end="17665990007t" region="region6" style="style0">- לא,
                על מה אתה מדבר?
            </ns0:p>
            <ns0:p xml:id="subtitle1091" begin="17649717501t" end="17665990007t" region="region2" style="style0">- זה לא
                כלום.
            </ns0:p>
            <ns0:p xml:id="subtitle1092" begin="17666407508t" end="17696855008t" region="region23" style="style0">אני
                פשוט הבטחתי שלא
            </ns0:p>
            <ns0:p xml:id="subtitle1093" begin="17666407508t" end="17696855008t" region="region63" style="style0">אעשה
                משהו טיפש
            </ns0:p>
            <ns0:p xml:id="subtitle1094" begin="17697272508t" end="17726467508t" region="region35" style="style0">ואשקיב
                אותו, וזה מה שעשיתי.
            </ns0:p>
            <ns0:p xml:id="subtitle1095" begin="17697272508t" end="17726467508t" region="region29" style="style0">- רגע,
                אתה שקיבת אותו?
            </ns0:p>
            <ns0:p xml:id="subtitle1096" begin="17726885008t" end="17738137500t" region="region46" style="style0">לא,
                לדעתי,
            </ns0:p>
            <ns0:p xml:id="subtitle1097" begin="17738555000t" end="17760245001t" region="region137" style="style0">הוא
                שקיב אותך.
            </ns0:p>
            <ns0:p xml:id="subtitle1098" begin="17738555000t" end="17760245001t" region="region152" style="style0">הוא
                תכנן
                אותך.
            </ns0:p>
            <ns0:p xml:id="subtitle1099" begin="17760662502t" end="17775682507t" region="region12" style="style0">- (
                chuckles )
            </ns0:p>
            <ns0:p xml:id="subtitle1100" begin="17760662502t" end="17775682507t" region="region136" style="style0">
                WHAT?
            </ns0:p>
            <ns0:p xml:id="subtitle1101" begin="17776100007t" end="17791945003t" region="region30" style="style0">- יש
                לו
                לדעת
            </ns0:p>
            <ns0:p xml:id="subtitle1102" begin="17776100007t" end="17791945003t" region="region46" style="style0">מה
                לואיס
                מסוגל,
            </ns0:p>
            <ns0:p xml:id="subtitle1103" begin="17792362503t" end="17806130007t" region="region3" style="style0">אז אם
                הוא
                לא
            </ns0:p>
            <ns0:p xml:id="subtitle1104" begin="17792362503t" end="17806130007t" region="region15" style="style0">שמע ל
                מה קרה
            </ns0:p>
            <ns0:p xml:id="subtitle1105" begin="17806547508t" end="17824897506t" region="region36" style="style0">זה
                כי
            </ns0:p>
            <ns0:p xml:id="subtitle1106" begin="17806547508t" end="17824897506t" region="region21" style="style0">הוא לא
                רצה.
            </ns0:p>
            <ns0:p xml:id="subtitle1107" begin="17825315006t" end="17839907501t" region="region25" style="style0">מייק,
                עליך
                לספר לו.
            </ns0:p>
            <ns0:p xml:id="subtitle1108" begin="17840325001t" end="17857015008t" region="region61" style="style0">- אני
                לא
                יודע כמה פעמים
            </ns0:p>
            <ns0:p xml:id="subtitle1109" begin="17840325001t" end="17857015008t" region="region184" style="style0">אתה
                נתקל
                בהרווי,
            </ns0:p>
            <ns0:p xml:id="subtitle1110" begin="17857432508t" end="17876617508t" region="region23" style="style0">אבל
                הוא
                לא בדיוק
            </ns0:p>
            <ns0:p xml:id="subtitle1111" begin="17857432508t" end="17876617508t" region="region185" style="style0">הסוג
                של
                האזנה.
            </ns0:p>
            <ns0:p xml:id="subtitle1112" begin="17877035008t" end="17892880003t" region="region158" style="style0">-
                נכון.
            </ns0:p>
            <ns0:p xml:id="subtitle1113" begin="17893297505t" end="17916657508t" region="region138" style="style0">אבל
                הוא
                גם האדם הראשון
            </ns0:p>
            <ns0:p xml:id="subtitle1114" begin="17893297505t" end="17916657508t" region="region37" style="style0">שיגיד
                לך
                לעולם לא לקבל תשובה של "לא"
            </ns0:p>
            <ns0:p xml:id="subtitle1115" begin="17917075008t" end="17948765000t" region="region138" style="style0">מאף
                אחד.
            </ns0:p>
            <ns0:p xml:id="subtitle1116" begin="17917075008t" end="17948765000t" region="region42" style="style0">-
                הרווי,
                איך אתה מרגיש
            </ns0:p>
            <ns0:p xml:id="subtitle1117" begin="18028427500t" end="18051787502t" region="region66" style="style0">היום
                הזה?
            </ns0:p>
            <ns0:p xml:id="subtitle1118" begin="18028427500t" end="18051787502t" region="region26" style="style0">- אני
                מוכן לשמוע
            </ns0:p>
            <ns0:p xml:id="subtitle1119" begin="18052205003t" end="18082652503t" region="region51" style="style0">את
                ההצעה
                לפשר, ג'ורג'.
            </ns0:p>
            <ns0:p xml:id="subtitle1120" begin="18052205003t" end="18082652503t" region="region27" style="style0">- חותך
                לקצה,
            </ns0:p>
            <ns0:p xml:id="subtitle1121" begin="18083070003t" end="18101420002t" region="region66" style="style0">אני
                אוהב
                את זה.
            </ns0:p>
            <ns0:p xml:id="subtitle1122" begin="18083070003t" end="18101420002t" region="region42" style="style0">- מה
                המספר?
            </ns0:p>
            <ns0:p xml:id="subtitle1123" begin="18101837502t" end="18136460007t" region="region0" style="style0">- עשרה
                מיליון.
            </ns0:p>
            <ns0:p xml:id="subtitle1124" begin="18136877508t" end="18160645001t" region="region26" style="style0">- זה
                חצי מה שביזינו
            </ns0:p>
            <ns0:p xml:id="subtitle1125" begin="18161062502t" end="18185257506t" region="region18" style="style0">בפיתוח
                הפרוטוטיפ.
            </ns0:p>
            <ns0:p xml:id="subtitle1126" begin="18161062502t" end="18185257506t" region="region46" style="style0">-
                ועשרה
                מיליון
            </ns0:p>
            <ns0:p xml:id="subtitle1127" begin="18185675006t" end="18204025005t" region="region30" style="style0">יותר
                ממה
                שתקבל,
            </ns0:p>
            <ns0:p xml:id="subtitle1128" begin="18185675006t" end="18204025005t" region="region32" style="style0">לפי
                הדרך
                שהרוח נושבת.
            </ns0:p>
            <ns0:p xml:id="subtitle1129" begin="18204442506t" end="18224045005t" region="region18" style="style0">- כן,
                והרוחות יכולות להשתנות.
            </ns0:p>
            <ns0:p xml:id="subtitle1130" begin="18204442506t" end="18224045005t" region="region67" style="style0">ואתה
                ואני יודעים
            </ns0:p>
            <ns0:p xml:id="subtitle1131" begin="18224462506t" end="18238220000t" region="region66" style="style0">את
                ההגבלה
            </ns0:p>
            <ns0:p xml:id="subtitle1132" begin="18224462506t" end="18238220000t" region="region26" style="style0">תיפול
                בערעור.
            </ns0:p>
            <ns0:p xml:id="subtitle1133" begin="18238637500t" end="18251987502t" region="region51" style="style0">- אני
                לא
                יודע
            </ns0:p>
            <ns0:p xml:id="subtitle1134" begin="18238637500t" end="18251987502t" region="region7" style="style0">כלום
                מהסוג הזה.
            </ns0:p>
            <ns0:p xml:id="subtitle1135" begin="18252405003t" end="18269502500t" region="region12" style="style0">- אז
                למה
                אתה מתקשר
            </ns0:p>
            <ns0:p xml:id="subtitle1136" begin="18252405003t" end="18269502500t" region="region32" style="style0">עם
                הצעה
                בכלל?
            </ns0:p>
            <ns0:p xml:id="subtitle1137" begin="18269920001t" end="18294950006t" region="region28" style="style0">- 15,
                זה הכי גבוה
            </ns0:p>
            <ns0:p xml:id="subtitle1138" begin="18269920001t" end="18294950006t" region="region32" style="style0">-
                בסדר,
                אני מניח שאני כבר כאן.
            </ns0:p>
            <ns0:p xml:id="subtitle1139" begin="18337495008t" end="18358757500t" region="region31" style="style0">- 20
                מיליון, זהו.
            </ns0:p>
            <ns0:p xml:id="subtitle1140" begin="18337495008t" end="18358757500t" region="region37" style="style0">ההצעה
                תקפה למשך 24 שעות.
            </ns0:p>
            <ns0:p xml:id="subtitle1141" begin="18359175000t" end="18386710007t" region="region18" style="style0">ואני
                בטוח
            </ns0:p>
            <ns0:p xml:id="subtitle1142" begin="18359175000t" end="18386710007t" region="region11" style="style0">שאין
                לי
                צורך להזכיר לך
            </ns0:p>
            <ns0:p xml:id="subtitle1143" begin="18387127508t" end="18402972503t" region="region27" style="style0">שאתה
                חייב
                משפטית
            </ns0:p>
            <ns0:p xml:id="subtitle1144" begin="18403390003t" end="18422992503t" region="region31" style="style0">להציג
                את זה ללקוח שלך.
            </ns0:p>
            <ns0:p xml:id="subtitle1145" begin="18403390003t" end="18422992503t" region="region13" style="style0">- האם
                הוא
                פנוי?
            </ns0:p>
            <ns0:p xml:id="subtitle1146" begin="18423410003t" end="18452605003t" region="region20" style="style0">- לא
                עבורך.
            </ns0:p>
            <ns0:p xml:id="subtitle1147" begin="18423410003t" end="18452605003t" region="region67" style="style0">- (
                נאנח
                )
            </ns0:p>
            <ns0:p xml:id="subtitle1148" begin="18560210001t" end="18576900007t" region="region63" style="style0">כמה
                הוא
                אמר לך?
            </ns0:p>
            <ns0:p xml:id="subtitle1149" begin="18577317508t" end="18593162503t" region="region26" style="style0">- הוא
                לא היה צריך לומר לי,
            </ns0:p>
            <ns0:p xml:id="subtitle1150" begin="18593580003t" end="18608590000t" region="region36" style="style0">- (
                sighs
                )
            </ns0:p>
            <ns0:p xml:id="subtitle1151" begin="18593580003t" end="18608590000t" region="region46" style="style0">אתה
                פגעת
                בו.
            </ns0:p>
            <ns0:p xml:id="subtitle1152" begin="18609007500t" end="18646552507t" region="region186" style="style0">-
                אולי הוא
                עשה אותו הדבר לי.
            </ns0:p>
            <ns0:p xml:id="subtitle1153" begin="18609007500t" end="18646552507t" region="region187" style="style0">-
                תכנס
                לשם.
            </ns0:p>
            <ns0:p xml:id="subtitle1154" begin="18646970007t" end="18666572507t" region="region43" style="style0">ותהיה
                זהיר.
            </ns0:p>
            <ns0:p xml:id="subtitle1155" begin="18646970007t" end="18666572507t" region="region27" style="style0">-
                הרווי.
            </ns0:p>
            <ns0:p xml:id="subtitle1156" begin="18707447508t" end="18719952500t" region="region2" style="style0">-
                דיברנו על זה, נכון?
            </ns0:p>
            <ns0:p xml:id="subtitle1157" begin="18735390006t" end="18759575000t" region="region2" style="style0">לפרוץ
                למשרד שלי.
            </ns0:p>
            <ns0:p xml:id="subtitle1158" begin="18799615000t" end="18819635000t" region="region5" style="style0">- אני
                לא
                עוזב
            </ns0:p>
            <ns0:p xml:id="subtitle1159" begin="18820052500t" end="18842995003t" region="region138" style="style0">עד
                שתשמע אותי.
            </ns0:p>
            <ns0:p xml:id="subtitle1160" begin="18820052500t" end="18842995003t" region="region15" style="style0">- רוצה
                להמר?
            </ns0:p>
            <ns0:p xml:id="subtitle1161" begin="18843412503t" end="18857597508t" region="region30" style="style0">- אתה
                חייב
                לי את זה.
            </ns0:p>
            <ns0:p xml:id="subtitle1162" begin="18843412503t" end="18857597508t" region="region9" style="style0">- אוי,
                אני
                חייב לך את זה?
            </ns0:p>
            <ns0:p xml:id="subtitle1163" begin="18858015008t" end="18876365006t" region="region31" style="style0">- כן.
            </ns0:p>
            <ns0:p xml:id="subtitle1164" begin="18858015008t" end="18876365006t" region="region37" style="style0">תראה,
                זה
                לא באשמתי, נכון?
            </ns0:p>
            <ns0:p xml:id="subtitle1165" begin="18876782507t" end="18896802507t" region="region129" style="style0">לואיס
                הכריח
                אותי לעשות את זה.
            </ns0:p>
            <ns0:p xml:id="subtitle1166" begin="18876782507t" end="18896802507t" region="region131" style="style0">-
                לואיס
                עשה, נכון.
            </ns0:p>
            <ns0:p xml:id="subtitle1167" begin="18897220007t" end="18911395001t" region="region43" style="style0">הוא שם
                רובה
                לראשך
            </ns0:p>
            <ns0:p xml:id="subtitle1168" begin="18897220007t" end="18911395001t" region="region11" style="style0">וכרך
                אותך
                לעשן גראס.
            </ns0:p>
            <ns0:p xml:id="subtitle1169" begin="18911812502t" end="18930162500t" region="region23" style="style0">- כן,
                הוא
                עשה.
            </ns0:p>
            <ns0:p xml:id="subtitle1170" begin="18911812502t" end="18930162500t" region="region185" style="style0">הוא
                שלף
                את מבחן הסמים--
            </ns0:p>
            <ns0:p xml:id="subtitle1171" begin="18930580001t" end="18948522508t" region="region41" style="style0">שבו
                נכשלתי, על דרך אגב.
            </ns0:p>
            <ns0:p xml:id="subtitle1172" begin="18930580001t" end="18948522508t" region="region15" style="style0">ואז
                הוא אמר לי
            </ns0:p>
            <ns0:p xml:id="subtitle1173" begin="18948930000t" end="18969785000t" region="region3" style="style0">שאם אני
                לא אעשה גראס
            </ns0:p>
            <ns0:p xml:id="subtitle1174" begin="18948930000t" end="18969785000t" region="region34" style="style0">כדי
                לעזור לו
                לקבל את הלקוח החדש הזה, הוא יפטר ממני.
            </ns0:p>
            <ns0:p xml:id="subtitle1175" begin="18970202500t" end="18985222506t" region="region25" style="style0">( נאנח
                )
            </ns0:p>
            <ns0:p xml:id="subtitle1176" begin="18985640006t" end="19003155003t" region="region51" style="style0">בוא
                על.
            </ns0:p>
            <ns0:p xml:id="subtitle1177" begin="18985640006t" end="19003155003t" region="region13" style="style0">זה לא
                כל כך שונה
            </ns0:p>
            <ns0:p xml:id="subtitle1178" begin="19003572503t" end="19035272506t" region="region4" style="style0">מלשאול
                אישה
                לשתות, או לא?
            </ns0:p>
            <ns0:p xml:id="subtitle1179" begin="19003572503t" end="19035272506t" region="region27" style="style0">- יש
                לך ולי
                עסקה.
            </ns0:p>
            <ns0:p xml:id="subtitle1180" begin="19056962507t" end="19069885000t" region="region38" style="style0">- אני
                מצטער.
            </ns0:p>
            <ns0:p xml:id="subtitle1181" begin="19056962507t" end="19069885000t" region="region158" style="style0">(
                מוזיקה
                מתנגנת )
            </ns0:p>
            <ns0:p xml:id="subtitle1182" begin="19070302500t" end="19084905005t" region="region137" style="style0">- ואם
                בפעם
                הבאה
            </ns0:p>
            <ns0:p xml:id="subtitle1183" begin="19070302500t" end="19084905005t" region="region150" style="style0">לואיס
                יבקש
                ממך לעשות משהו
            </ns0:p>
            <ns0:p xml:id="subtitle1184" begin="19085322506t" end="19102837502t" region="region129" style="style0">
                שאמרתי לך
                שלא לעשות,
            </ns0:p>
            <ns0:p xml:id="subtitle1185" begin="19085322506t" end="19102837502t" region="region15" style="style0">מה
                אז?
            </ns0:p>
            <ns0:p xml:id="subtitle1186" begin="19103255003t" end="19120352500t" region="region31" style="style0">-
                אמרתי לך
                שאין לי ברירה.
            </ns0:p>
            <ns0:p xml:id="subtitle1187" begin="19103255003t" end="19120352500t" region="region2" style="style0">- אוי,
                כי היה לו רובה לראשך.
            </ns0:p>
            <ns0:p xml:id="subtitle1188" begin="19120770001t" end="19138712508t" region="region35" style="style0">-
                כן!
            </ns0:p>
            <ns0:p xml:id="subtitle1189" begin="19120770001t" end="19138712508t" region="region27" style="style0">- מה
                האפשרויות
                שלך
            </ns0:p>
            <ns0:p xml:id="subtitle1190" begin="19139120000t" end="19163732503t" region="region28" style="style0">אם
                מישהו
            </ns0:p>
            <ns0:p xml:id="subtitle1191" begin="19139120000t" end="19163732503t" region="region7" style="style0">מה
                אז?
            </ns0:p>
            <ns0:p xml:id="subtitle1192" begin="19164150003t" end="19183335003t" region="region30" style="style0">-
                אמרתי
                לך ש
            </ns0:p>
            <ns0:p xml:id="subtitle1193" begin="19164150003t" end="19183335003t" region="region11" style="style0">אין לי
                ברירה.
            </ns0:p>
            <ns0:p xml:id="subtitle1194" begin="19183752503t" end="19195432506t" region="region43" style="style0">- אה,
                כי
            </ns0:p>
            <ns0:p xml:id="subtitle1195" begin="19183752503t" end="19195432506t" region="region13" style="style0">היה לו
                רובה לראשך.
            </ns0:p>
            <ns0:p xml:id="subtitle1196" begin="19195850006t" end="19208782508t" region="region54" style="style0">-
                כן!
            </ns0:p>
            <ns0:p xml:id="subtitle1197" begin="19195850006t" end="19208782508t" region="region15" style="style0">- מה
                אפשר
                לעשות
            </ns0:p>
            <ns0:p xml:id="subtitle1198" begin="19209190000t" end="19224210003t" region="region6" style="style0">אם
                מישהו
            </ns0:p>
            <ns0:p xml:id="subtitle1199" begin="19209190000t" end="19224210003t" region="region11" style="style0">מניח
                רובה
                לראשך?
            </ns0:p>
            <ns0:p xml:id="subtitle1200" begin="19224627505t" end="19237977508t" region="region145" style="style0">- ?מה
                אתה מדבר על זה
            </ns0:p>
            <ns0:p xml:id="subtitle1201" begin="19224627505t" end="19237977508t" region="region176" style="style0">אתה
                עושה
                מה שהם אומרים
            </ns0:p>
            <ns0:p xml:id="subtitle1202" begin="19238395008t" end="19253405003t" region="region188" style="style0">או
                שהם
                יורים בך.
            </ns0:p>
            <ns0:p xml:id="subtitle1203" begin="19238395008t" end="19253405003t" region="region189" style="style0">-
                לא נכון.
            </ns0:p>
            <ns0:p xml:id="subtitle1204" begin="19253822503t" end="19277182507t" region="region30" style="style0">אתה
                לוקח
                את הרובה,
            </ns0:p>
            <ns0:p xml:id="subtitle1205" begin="19253822503t" end="19277182507t" region="region34" style="style0">או
                שאתה
                מוציא רובה גדולה יותר,
            </ns0:p>
            <ns0:p xml:id="subtitle1206" begin="19277600007t" end="19290522500t" region="region11" style="style0">או
                שאתה
                מקרא את הבלאף שלהם.
            </ns0:p>
            <ns0:p xml:id="subtitle1207" begin="19290940001t" end="19317222507t" region="region23" style="style0">או
                שאתה עושה
                כל אחד מ
            </ns0:p>
            <ns0:p xml:id="subtitle1208" begin="19290940001t" end="19317222507t" region="region19" style="style0">146
                דברים
                אחרים.
            </ns0:p>
            <ns0:p xml:id="subtitle1209" begin="19317640007t" end="19343077502t" region="region147" style="style0">אם
                אינך
                יכול לחשוב בעצמך,
            </ns0:p>
            <ns0:p xml:id="subtitle1210" begin="19317640007t" end="19343077502t" region="region65" style="style0">אתה לא
                מתאים לזה.
            </ns0:p>
            <ns0:p xml:id="subtitle1211" begin="19343495003t" end="19366855006t" region="region4" style="style0">- לא,
                אני
                יכול ואני.
            </ns0:p>
            <ns0:p xml:id="subtitle1212" begin="19343495003t" end="19366855006t" region="region34" style="style0">תראה,
                עשיתי
                מה שהייתי צריך לעשות.
            </ns0:p>
            <ns0:p xml:id="subtitle1213" begin="19367272507t" end="19386457506t" region="region51" style="style0">עשיתי
                את
                המיטב
            </ns0:p>
            <ns0:p xml:id="subtitle1214" begin="19367272507t" end="19386457506t" region="region37" style="style0">ממצב
                רע.
            </ns0:p>
            <ns0:p xml:id="subtitle1215" begin="19386875006t" end="19402302502t" region="region29" style="style0">זה מה
                שאני
                טוב בו--
            </ns0:p>
            <ns0:p xml:id="subtitle1216" begin="19402720002t" end="19417740007t" region="region28" style="style0">זה מה
                שאתה שכרת אותי
            </ns0:p>
            <ns0:p xml:id="subtitle1217" begin="19402720002t" end="19417740007t" region="region190" style="style0">לעשות
                ב
                המקום הראשון.
            </ns0:p>
            <ns0:p xml:id="subtitle1218" begin="19418157508t" end="19439420000t" region="region51" style="style0">- כן,
                שכרתי אותך
            </ns0:p>
            <ns0:p xml:id="subtitle1219" begin="19418157508t" end="19439420000t" region="region13" style="style0">ואני
                מצפה לנאמנות שלך.
            </ns0:p>
            <ns0:p xml:id="subtitle1220" begin="19439837500t" end="19461527501t" region="region28" style="style0">אז אם
                לואיס מבקש ממך לעשות
            </ns0:p>
            <ns0:p xml:id="subtitle1221" begin="19439837500t" end="19461527501t" region="region0" style="style0">משהו
                כזה,
            </ns0:p>
            <ns0:p xml:id="subtitle1222" begin="19461945001t" end="19481130001t" region="region4" style="style0">אתה
                מגיע אליי
                ראשון.
            </ns0:p>
            <ns0:p xml:id="subtitle1223" begin="19461945001t" end="19481130001t" region="region42" style="style0">- אה,
                נכון.
            </ns0:p>
            <ns0:p xml:id="subtitle1224" begin="19481547501t" end="19502820002t" region="region66" style="style0">כמו כש
                הגעתי אליך
            </ns0:p>
            <ns0:p xml:id="subtitle1225" begin="19481547501t" end="19502820002t" region="region2" style="style0">ואמרת
                לי
            </ns0:p>
            <ns0:p xml:id="subtitle1226" begin="19503237502t" end="19519500000t" region="region51" style="style0">שאני
                לא יכול לשבת
            </ns0:p>
            <ns0:p xml:id="subtitle1227" begin="19503237502t" end="19519500000t" region="region13" style="style0">בשולחן
                המבוגרים, נכון.
            </ns0:p>
            <ns0:p xml:id="subtitle1228" begin="19519917500t" end="19539520000t" region="region8" style="style0">אני
                זוכר
                את זה.
            </ns0:p>
            <ns0:p xml:id="subtitle1229" begin="19519917500t" end="19539520000t" region="region9" style="style0">או-או
                כש
                אמרת לי
            </ns0:p>
            <ns0:p xml:id="subtitle1230" begin="19539937500t" end="19557045006t" region="region4" style="style0">ללכת
                להגיש
                את הפטנט
            </ns0:p>
            <ns0:p xml:id="subtitle1231" begin="19539937500t" end="19557045006t" region="region32" style="style0">בעצמי,
                כזה?
            </ns0:p>
            <ns0:p xml:id="subtitle1232" begin="19557462507t" end="19575395005t" region="region31" style="style0">-
                סליחה
                ?
            </ns0:p>
            <ns0:p xml:id="subtitle1233" begin="19557462507t" end="19575395005t" region="region17" style="style0">- אתה
                יודע
                מה?
            </ns0:p>
            <ns0:p xml:id="subtitle1234" begin="19575812506t" end="19606677506t" region="region3" style="style0">אולי זה
                הזמן
            </ns0:p>
            <ns0:p xml:id="subtitle1235" begin="19575812506t" end="19606677506t" region="region25" style="style0">שתראה
                לי קצת נאמנות.
            </ns0:p>
            <ns0:p xml:id="subtitle1236" begin="19627950007t" end="19645047505t" region="region34" style="style0">- ?אתה
                יודע כמה זמן
                עבר
            </ns0:p>
            <ns0:p xml:id="subtitle1237" begin="19645465005t" end="19672155001t" region="region31" style="style0">עד ש
                קיבלתי
            </ns0:p>
            <ns0:p xml:id="subtitle1238" begin="19645465005t" end="19672155001t" region="region13" style="style0">לשבת ב
                שולחן המבוגרים?
            </ns0:p>
            <ns0:p xml:id="subtitle1239" begin="19672572502t" end="19685922506t" region="region43" style="style0">- זה
                היה
                כאשר
            </ns0:p>
            <ns0:p xml:id="subtitle1240" begin="19672572502t" end="19685922506t" region="region34" style="style0">הבאתי
                את הלקוח הראשון שלי,
            </ns0:p>
            <ns0:p xml:id="subtitle1241" begin="19686340006t" end="19708865008t" region="region23" style="style0">שאני
