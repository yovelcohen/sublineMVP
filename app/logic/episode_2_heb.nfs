            <ns0:p xml:id="subtitle288" begin="4211715006t" end="4237152502t" region="region27" style="style0">- ( נשימה
                עמוקה )
            </ns0:p>
            <ns0:p xml:id="subtitle289" begin="4250085005t" end="4265095000t" region="region33" style="style0">- ( sighs
                )
            </ns0:p>
            <ns0:p xml:id="subtitle290" begin="4265512500t" end="4323912508t" region="region55" style="style0">אני יכול
                להתקשה קצת...
            </ns0:p>
            <ns0:p xml:id="subtitle291" begin="4265512500t" end="4323912508t" region="region13" style="style0">מצחיק.
            </ns0:p>
            <ns0:p xml:id="subtitle292" begin="4324320000t" end="4349767505t" region="region130" style="style0">- ( צוחק
                )
            </ns0:p>
            <ns0:p xml:id="subtitle293" begin="4324320000t" end="4349767505t" region="region26" style="style0">( מנקה
                גרון )
            </ns0:p>
            <ns0:p xml:id="subtitle294" begin="4350185005t" end="4364360000t" region="region30" style="style0">( clears
                throat )
            </ns0:p>
            <ns0:p xml:id="subtitle295" begin="4350185005t" end="4364360000t" region="region11" style="style0">זה אומר
                שאני כאן לעזור.
            </ns0:p>
            <ns0:p xml:id="subtitle296" begin="4364777500t" end="4396060001t" region="region29" style="style0">- בסדר,
                כל
                הכבוד.
            </ns0:p>
            <ns0:p xml:id="subtitle297" begin="4419837505t" end="4443197508t" region="region21" style="style0">אז אתה
                יכול
                לעזור לי עכשיו?
            </ns0:p>
            <ns0:p xml:id="subtitle298" begin="4443615008t" end="4459042503t" region="region15" style="style0">אז אתה
                יכול\n לעזור לי עכשיו?\n
            </ns0:p>
            <ns0:p xml:id="subtitle299" begin="4474887500t" end="4502840007t" region="region19" style="style0">- כבר\n
                עשיתי.\n
            </ns0:p>
            <ns0:p xml:id="subtitle300" begin="4503257508t" end="4526607501t" region="region131" style="style0">-
