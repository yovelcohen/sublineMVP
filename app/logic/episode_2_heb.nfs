        <ns0:div>
            <ns0:p xml:id="subtitle1" begin="15020003t" end="37545006t" region="region0" style="style0">( percussive
                music
                )
            </ns0:p>
            <ns0:p xml:id="subtitle2" begin="37962507t" end="98857508t" region="region1" style="style0">♪ ♪</ns0:p>
            <ns0:p xml:id="subtitle3" begin="99275008t" end="123042502t" region="region2" style="style0">(הלוח מצטט)
            </ns0:p>
            <ns0:p xml:id="subtitle4" begin="123460002t" end="137227506t" region="region3" style="style0">- מה זה, שלוש
                ברצף?\n
            </ns0:p>
            <ns0:p xml:id="subtitle5" begin="123460002t" end="137227506t" region="region2" style="style0">שלוש ברצף?\n
            </ns0:p>
            <ns0:p xml:id="subtitle6" begin="137645006t" end="156412506t" region="region4" style="style0">- זה יהיה
                ארבע, בעצם.\n
            </ns0:p>
            <ns0:p xml:id="subtitle7" begin="137645006t" end="156412506t" region="region5" style="style0">בכלל.</ns0:p>
            <ns0:p xml:id="subtitle8" begin="156830006t" end="171422500t" region="region6" style="style0">- אה,
                אלוהים.
            </ns0:p>
            <ns0:p xml:id="subtitle9" begin="156830006t" end="171422500t" region="region7" style="style0">- בוא נתחיל.
            </ns0:p>
            <ns0:p xml:id="subtitle10" begin="171840001t" end="193947502t" region="region8" style="style0">מה אתה אומר,
                נלך חמישה מתוך
            </ns0:p>
            <ns0:p xml:id="subtitle11" begin="171840001t" end="193947502t" region="region9" style="style0">תשע?
            </ns0:p>
            <ns0:p xml:id="subtitle12" begin="194365003t" end="218977508t" region="region10" style="style0">- כך שתוכל
                להמשיך להתעלל בי?
            </ns0:p>
            <ns0:p xml:id="subtitle13" begin="194365003t" end="218977508t" region="region11" style="style0">לא, תודה.
                סיימתי.
            </ns0:p>
            <ns0:p xml:id="subtitle14" begin="219395008t" end="240657500t" region="region12" style="style0">- אה, בוא
                נתחיל.
            </ns0:p>
            <ns0:p xml:id="subtitle15" begin="219395008t" end="240657500t" region="region0" style="style0">אני אתן לך
                שלוש.
            </ns0:p>
            <ns0:p xml:id="subtitle16" begin="241075000t" end="253172502t" region="region13" style="style0">- זמן המשחק
                נגמר, וויאט.
            </ns0:p>
            <ns0:p xml:id="subtitle17" begin="253590002t" end="267775006t" region="region14" style="style0">הם כאן.
            </ns0:p>
            <ns0:p xml:id="subtitle18" begin="253590002t" end="267775006t" region="region2" style="style0">- הם כאן?
            </ns0:p>
            <ns0:p xml:id="subtitle19" begin="268192507t" end="281115000t" region="region8" style="style0">- אתה יכול
                להתקין
                ב
            </ns0:p>
            <ns0:p xml:id="subtitle20" begin="268192507t" end="281115000t" region="region15" style="style0">חדר הכנסות.
            </ns0:p>
            <ns0:p xml:id="subtitle21" begin="281532500t" end="294465003t" region="region16" style="style0">- כן.
            </ns0:p>
            <ns0:p xml:id="subtitle22" begin="281532500t" end="294465003t" region="region17" style="style0">לא, אני
                פשוט...
            </ns0:p>
            <ns0:p xml:id="subtitle23" begin="294882503t" end="324912503t" region="region3" style="style0">- להתקין ב
            </ns0:p>
            <ns0:p xml:id="subtitle24" begin="294882503t" end="324912503t" region="region0" style="style0">חדר הכנסות.
            </ns0:p>
            <ns0:p xml:id="subtitle25" begin="325330003t" end="356195005t" region="region18" style="style0">אתה נתת לו
                לנצח, נכון?
            </ns0:p>
            <ns0:p xml:id="subtitle26" begin="325330003t" end="356195005t" region="region19" style="style0">- זה לא היה
                קל.
            </ns0:p>
            <ns0:p xml:id="subtitle27" begin="356612506t" end="377050006t" region="region20" style="style0">הבחור יש לו
                את
                התיאום
            </ns0:p>
            <ns0:p xml:id="subtitle28" begin="356612506t" end="377050006t" region="region21" style="style0">של ילד בן
                שנתיים.
            </ns0:p>
            <ns0:p xml:id="subtitle29" begin="377467506t" end="399992508t" region="region10" style="style0">- אני יודע.
                הפעם האחרונה
                שהון הון סייעו,
            </ns0:p>
            <ns0:p xml:id="subtitle30" begin="377467506t" end="399992508t" region="region22" style="style0">אני הבאתי
                אותם
                לאימון בוקס
            </ns0:p>
            <ns0:p xml:id="subtitle31" begin="400400000t" end="419177508t" region="region23" style="style0">כדי להרגיע
                אותם.
            </ns0:p>
            <ns0:p xml:id="subtitle32" begin="400400000t" end="419177508t" region="region2" style="style0">בגדול, הייתה
                לי
                צורך להתכות
            </ns0:p>
            <ns0:p xml:id="subtitle33" begin="419595008t" end="441275000t" region="region24" style="style0">בפנים כדי
                לתת להם לנצח.
            </ns0:p>
            <ns0:p xml:id="subtitle34" begin="419595008t" end="441275000t" region="region25" style="style0">- ( צחוק )
            </ns0:p>
            <ns0:p xml:id="subtitle35" begin="441692500t" end="457130006t" region="region26" style="style0">אני לא מבין.
            </ns0:p>
            <ns0:p xml:id="subtitle36" begin="457547506t" end="476315005t" region="region3" style="style0">אני מתכוון,
                אם הוא
                כל כך גאון,
            </ns0:p>
            <ns0:p xml:id="subtitle37" begin="457547506t" end="476315005t" region="region27" style="style0">למה הוא כל
                כך
                נבוך
            </ns0:p>
            <ns0:p xml:id="subtitle38" begin="476732506t" end="495917505t" region="region28" style="style0">מסביב
                למשקיעים?
            </ns0:p>
            <ns0:p xml:id="subtitle39" begin="476732506t" end="495917505t" region="region29" style="style0">- אה, אתה
                מבלה
            </ns0:p>
            <ns0:p xml:id="subtitle40" begin="496335005t" end="516772506t" region="region3" style="style0">שנתיים עובד
                על
                טלפון לוויין
            </ns0:p>
            <ns0:p xml:id="subtitle41" begin="496335005t" end="516772506t" region="region0" style="style0">שמתאים לכיס
                שלך
            </ns0:p>
            <ns0:p xml:id="subtitle42" begin="517190006t" end="529287508t" region="region30" style="style0">ואז המרת
                עליו את החברה.
            </ns0:p>
            <ns0:p xml:id="subtitle43" begin="517190006t" end="529287508t" region="region11" style="style0">גם אתה היית
                קצת
            </ns0:p>
            <ns0:p xml:id="subtitle44" begin="529705008t" end="543462502t" region="region31" style="style0">נבוך, אני
                חושב.
            </ns0:p>
            <ns0:p xml:id="subtitle45" begin="529705008t" end="543462502t" region="region21" style="style0">- הדגם...
            </ns0:p>
            <ns0:p xml:id="subtitle46" begin="543880002t" end="563065001t" region="region30" style="style0">אני לא מוצא
                אותו.
            </ns0:p>
            <ns0:p xml:id="subtitle47" begin="543880002t" end="563065001t" region="region32" style="style0">אני לא יודע
                איפה אני--
            </ns0:p>
            <ns0:p xml:id="subtitle48" begin="563482502t" end="579755008t" region="region21" style="style0">- אתה מתכוון
                לזה?
            </ns0:p>
            <ns0:p xml:id="subtitle49" begin="580172508t" end="599775008t" region="region8" style="style0">- ( נושם )
            </ns0:p>
            <ns0:p xml:id="subtitle50" begin="580172508t" end="599775008t" region="region9" style="style0">אני לא יודע
                איפה אני--
            </ns0:p>
            <ns0:p xml:id="subtitle51" begin="600192508t" end="619377508t" region="region8" style="style0">- אתה מתכוון
                לזה?
            </ns0:p>
            <ns0:p xml:id="subtitle52" begin="600192508t" end="619377508t" region="region33" style="style0">- ( נושם )
            </ns0:p>
            <ns0:p xml:id="subtitle53" begin="619795008t" end="634387502t" region="region31" style="style0">אני לא
                יודע
            </ns0:p>
            <ns0:p xml:id="subtitle54" begin="619795008t" end="634387502t" region="region11" style="style0">מה הייתי
                עושה
                בלעדיך.
            </ns0:p>
            <ns0:p xml:id="subtitle55" begin="634805003t" end="659000007t" region="region30" style="style0">- לכשל
                בצורה מחורבנת.
            </ns0:p>
            <ns0:p xml:id="subtitle56" begin="634805003t" end="659000007t" region="region7" style="style0">- אני
                הייתי.
            </ns0:p>
            <ns0:p xml:id="subtitle57" begin="659417508t" end="685272503t" region="region8" style="style0">- האם זה
                ה...
            </ns0:p>
            <ns0:p xml:id="subtitle58" begin="659417508t" end="685272503t" region="region34" style="style0">- דגם של 20
                מיליון
                דולר, כן.
            </ns0:p>
            <ns0:p xml:id="subtitle59" begin="685690003t" end="702370001t" region="region35" style="style0">ואתה אפילו
                לא
            </ns0:p>
            <ns0:p xml:id="subtitle60" begin="685690003t" end="702370001t" region="region25" style="style0">מקבל לראות
                את המכונית הזו
            </ns0:p>
            <ns0:p xml:id="subtitle61" begin="702787501t" end="719477508t" region="region36" style="style0">עד אחרי
            </ns0:p>
            <ns0:p xml:id="subtitle62" begin="702787501t" end="719477508t" region="region11" style="style0">שנסגור את
                העסקה היום.
            </ns0:p>
            <ns0:p xml:id="subtitle63" begin="719895008t" end="741575000t" region="region12" style="style0">- וואו, הם
            </ns0:p>
            <ns0:p xml:id="subtitle64" begin="719895008t" end="741575000t" region="region0" style="style0">כמו חיילי
                סערה.
            </ns0:p>
            <ns0:p xml:id="subtitle65" begin="741992500t" end="767022506t" region="region8" style="style0">- חיילי
                סערה
            </ns0:p>
            <ns0:p xml:id="subtitle66" begin="741992500t" end="767022506t" region="region37" style="style0">לא כותבים
                צ'קים.
            </ns0:p>
            <ns0:p xml:id="subtitle67" begin="767440006t" end="784955003t" region="region23" style="style0">עכשיו כל מה
                ש
                עליך לעשות
            </ns0:p>
            <ns0:p xml:id="subtitle68" begin="767440006t" end="784955003t" region="region19" style="style0">זה להיכנס
                שם,
            </ns0:p>
            <ns0:p xml:id="subtitle69" begin="785372503t" end="800392508t" region="region38" style="style0">להראות להם
            </ns0:p>
            <ns0:p xml:id="subtitle70" begin="785372503t" end="800392508t" region="region9" style="style0">איך הטלפון
                עובד...
            </ns0:p>
            <ns0:p xml:id="subtitle71" begin="800800000t" end="832082500t" region="region39" style="style0">- נכון,
                ו-SUNTECH DIGITAL
            </ns0:p>
            <ns0:p xml:id="subtitle72" begin="800800000t" end="832082500t" region="region40" style="style0">תהיה כוח חדש
                בתחום
            </ns0:p>
            <ns0:p xml:id="subtitle73" begin="832500001t" end="857947506t" region="region41" style="style0">התקשורת
                הגלובלית.
            </ns0:p>
            <ns0:p xml:id="subtitle74" begin="832500001t" end="857947506t" region="region15" style="style0">- בוא נפגוע
                בהם בצורה קשה.
            </ns0:p>
            <ns0:p xml:id="subtitle75" begin="876297505t" end="901735000t" region="region42" style="style0">- בסדר.
            </ns0:p>
            <ns0:p xml:id="subtitle76" begin="902152500t" end="918007506t" region="region43" style="style0">- חכה, חכה.
            </ns0:p>
            <ns0:p xml:id="subtitle77" begin="902152500t" end="918007506t" region="region44" style="style0">לאן אתה
                חושב שאתה הולך?
            </ns0:p>
            <ns0:p xml:id="subtitle78" begin="918425006t" end="937192506t" region="region45" style="style0">- לתוך ה
                חדר
            </ns0:p>
            <ns0:p xml:id="subtitle79" begin="918425006t" end="937192506t" region="region17" style="style0">עם ה
                אנשים.
            </ns0:p>
            <ns0:p xml:id="subtitle80" begin="937610006t" end="963047501t" region="region3" style="style0">- לא נכון, זה
            </ns0:p>
            <ns0:p xml:id="subtitle81" begin="937610006t" end="963047501t" region="region46" style="style0">שולחן
                המבוגרים
                שם בתוך.
            </ns0:p>
            <ns0:p xml:id="subtitle82" begin="963465001t" end="980155008t" region="region3" style="style0">ואתה עדיין לא
                רכשת את
            </ns0:p>
            <ns0:p xml:id="subtitle83" begin="963465001t" end="980155008t" region="region46" style="style0">EARNED THE
                PRIVILEGE YET.
            </ns0:p>
            <ns0:p xml:id="subtitle84" begin="980572508t" end="992660001t" region="region12" style="style0">- אבל שיחקתי
            </ns0:p>
            <ns0:p xml:id="subtitle85" begin="980572508t" end="992660001t" region="region32" style="style0">איר הוקי עם
                הוא--
            </ns0:p>
            <ns0:p xml:id="subtitle86" begin="993077501t" end="1006427505t" region="region47" style="style0">- אתה צריך
                לחזור
            </ns0:p>
            <ns0:p xml:id="subtitle87" begin="993077501t" end="1006427505t" region="region48" style="style0">למשרד
            </ns0:p>
            <ns0:p xml:id="subtitle88" begin="1006845005t" end="1021437500t" region="region49" style="style0">ולהגיש
            </ns0:p>
            <ns0:p xml:id="subtitle89" begin="1006845005t" end="1021437500t" region="region50" style="style0">פטנט עבור
                הטלפון.
            </ns0:p>
            <ns0:p xml:id="subtitle90" begin="1021855000t" end="1040632508t" region="region51" style="style0">- פטנט?
                אני
                לא
            </ns0:p>
            <ns0:p xml:id="subtitle91" begin="1021855000t" end="1040632508t" region="region13" style="style0">יודע איך
                להגיש
                פטנט.
            </ns0:p>
            <ns0:p xml:id="subtitle92" begin="1041039999t" end="1058982507t" region="region8" style="style0">- תבין את
                זה
                בעצמך.
            </ns0:p>
            <ns0:p xml:id="subtitle93" begin="1041039999t" end="1058982507t" region="region13" style="style0">- האם
                אנחנו יכולים
                לעשות את זה אחרי--
            </ns0:p>
            <ns0:p xml:id="subtitle94" begin="1059400007t" end="1079002507t" region="region52" style="style0">- לא, ואם
                תמשיך
                לדבר,
            </ns0:p>
            <ns0:p xml:id="subtitle95" begin="1059400007t" end="1079002507t" region="region29" style="style0">אני אתחיל
                לחייב אותך.
            </ns0:p>
            <ns0:p xml:id="subtitle96" begin="1079420007t" end="1096100003t" region="region36" style="style0">והזמן שלי
            </ns0:p>
            <ns0:p xml:id="subtitle97" begin="1079420007t" end="1096100003t" region="region0" style="style0">עולה 1,000
                דולר
                לשעה.
            </ns0:p>
            <ns0:p xml:id="subtitle98" begin="1096517505t" end="1119877508t" region="region28" style="style0">- אבל האם
                זה הזמן שלנו,
            </ns0:p>
            <ns0:p xml:id="subtitle99" begin="1096517505t" end="1119877508t" region="region5" style="style0">מר. הנד?
            </ns0:p>
            <ns0:p xml:id="subtitle100" begin="1134887502t" end="1151567500t" region="region0" style="style0">- אני עומד
                מתוקן.
            </ns0:p>
            <ns0:p xml:id="subtitle101" begin="1151985000t" end="1168675006t" region="region3" style="style0">
                <ns0:span style="style1">הזמנים שלך מהירים</ns0:span>
            </ns0:p>
            <ns0:p xml:id="subtitle102" begin="1151985000t" end="1168675006t" region="region53" style="style0">
                <ns0:span style="style1">בבית הספר התיכון רידג'מונט</ns0:span>
                QUOTE,
            </ns0:p>
            <ns0:p xml:id="subtitle103" begin="1169092507t" end="1195782503t" region="region3" style="style0">זה מוכיח
            </ns0:p>
            <ns0:p xml:id="subtitle104" begin="1169092507t" end="1195782503t" region="region13" style="style0">שאתה שייך
                לשולחן המבוגרים.
            </ns0:p>
            <ns0:p xml:id="subtitle105" begin="1196200003t" end="1219977508t" region="region54" style="style0">- HEY,
            </ns0:p>
            <ns0:p xml:id="subtitle106" begin="1196200003t" end="1219977508t" region="region32" style="style0">זה סרט
                מעולה.
            </ns0:p>
            <ns0:p xml:id="subtitle107" begin="1220395008t" end="1239162507t" region="region31" style="style0">והוא
            </ns0:p>
            <ns0:p xml:id="subtitle108" begin="1220395008t" end="1239162507t" region="region21" style="style0">דיבר
                לדור...
            </ns0:p>
            <ns0:p xml:id="subtitle109" begin="1344680002t" end="1375545003t" region="region26" style="style0">הי, אתה
                תפוס?
            </ns0:p>
            <ns0:p xml:id="subtitle110" begin="1408080006t" end="1429770007t" region="region8" style="style0">- בסדר,
                האם
            </ns0:p>
            <ns0:p xml:id="subtitle111" begin="1408080006t" end="1429770007t" region="region25" style="style0">אי פעם
                דיינת פטנט לפני?
            </ns0:p>
            <ns0:p xml:id="subtitle112" begin="1430187508t" end="1444780002t" region="region23" style="style0">רק
                חזרתי
            </ns0:p>
            <ns0:p xml:id="subtitle113" begin="1430187508t" end="1444780002t" region="region11" style="style0">מהפגישה
                הזו עם הארווי
            </ns0:p>
            <ns0:p xml:id="subtitle114" begin="1445197502t" end="1461052508t" region="region3" style="style0">והוא
                רוצה
            </ns0:p>
            <ns0:p xml:id="subtitle115" begin="1445197502t" end="1461052508t" region="region0" style="style0">שאני אפטיר
                את הפטנט הזה,
            </ns0:p>
            <ns0:p xml:id="subtitle116" begin="1461877500t" end="1478150006t" region="region55" style="style0">אבל אין
                לי שום
            </ns0:p>
            <ns0:p xml:id="subtitle117" begin="1461877500t" end="1478150006t" region="region13" style="style0">רעיון
                איך
            </ns0:p>
            <ns0:p xml:id="subtitle118" begin="1478567506t" end="1508597506t" region="region8" style="style0">הניירות
                האלה נראים.
            </ns0:p>
            <ns0:p xml:id="subtitle119" begin="1478567506t" end="1508597506t" region="region11" style="style0">אז... כל
                עזרה
            </ns0:p>
            <ns0:p xml:id="subtitle120" begin="1509015006t" end="1529870007t" region="region2" style="style0">(הדלת
                נסגרת)
            </ns0:p>
            <ns0:p xml:id="subtitle121" begin="1530287508t" end="1546967505t" region="region51" style="style0">הארווי
            </ns0:p>
            <ns0:p xml:id="subtitle122" begin="1530287508t" end="1546967505t" region="region11" style="style0">קרא לך
            </ns0:p>
            <ns0:p xml:id="subtitle123" begin="1642475000t" end="1665417502t" region="region34" style="style0">- אם זה
                לא
            </ns0:p>
            <ns0:p xml:id="subtitle124" begin="1665835003t" end="1687107505t" region="region30" style="style0">הבחור הזה
                של הארווי.
            </ns0:p>
            <ns0:p xml:id="subtitle125" begin="1665835003t" end="1687107505t" region="region2" style="style0">פרויקט של
                הארווי, נכון?
            </ns0:p>
            <ns0:p xml:id="subtitle126" begin="1687525005t" end="1711302508t" region="region56" style="style0">מה שמך?
            </ns0:p>
            <ns0:p xml:id="subtitle127" begin="1687525005t" end="1711302508t" region="region21" style="style0">- מייק,
                מייק
            </ns0:p>
            <ns0:p xml:id="subtitle128" begin="1711709999t" end="1725477502t" region="region17" style="style0">-
                גרגורי
            </ns0:p>
            <ns0:p xml:id="subtitle129" begin="1725895003t" end="1742157500t" region="region57" style="style0">בון.
            </ns0:p>
            <ns0:p xml:id="subtitle130" begin="1742575000t" end="1770945008t" region="region58" style="style0">אז מה אתה
                עושה כאן?
            </ns0:p>
            <ns0:p xml:id="subtitle131" begin="1742575000t" end="1770945008t" region="region59" style="style0">האם
                חתמת
            </ns0:p>
            <ns0:p xml:id="subtitle132" begin="1742575000t" end="1770945008t" region="region60" style="style0">על עסקה
                גדולה
            </ns0:p>
            <ns0:p xml:id="subtitle133" begin="1771362508t" end="1794295001t" region="region18" style="style0">הצהרה
                בעיר?
            </ns0:p>
            <ns0:p xml:id="subtitle134" begin="1771362508t" end="1794295001t" region="region46" style="style0">- ובכל
                זאת לא ראיתי
            </ns0:p>
            <ns0:p xml:id="subtitle135" begin="1794712502t" end="1808062506t" region="region34" style="style0">את הזכות
                לשולחן המבוגרים?
            </ns0:p>
            <ns0:p xml:id="subtitle136" begin="1840597508t" end="1859365006t" region="region8" style="style0">- ובמקום
            </ns0:p>
            <ns0:p xml:id="subtitle137" begin="1840597508t" end="1859365006t" region="region11" style="style0">לצפות
                בסגירת העסקה,
            </ns0:p>
            <ns0:p xml:id="subtitle138" begin="1859782507t" end="1878967506t" region="region46" style="style0">אני צריך
                לדיין
            </ns0:p>
            <ns0:p xml:id="subtitle139" begin="1879385006t" end="1899405006t" region="region37" style="style0">- כן,
                אלו
            </ns0:p>
            <ns0:p xml:id="subtitle140" begin="1913997501t" end="1939445006t" region="region61" style="style0">- חכה,
            </ns0:p>
            <ns0:p xml:id="subtitle141" begin="1913997501t" end="1939445006t" region="region0" style="style0">אתה--אתה
                עשית
            </ns0:p>
            <ns0:p xml:id="subtitle142" begin="1939862507t" end="1964882502t" region="region36" style="style0">אחד כזה
                לפני?
            </ns0:p>
            <ns0:p xml:id="subtitle143" begin="1939862507t" end="1964882502t" region="region62" style="style0">WHY?
            </ns0:p>
            <ns0:p xml:id="subtitle144" begin="2011185008t" end="2030370007t" region="region19" style="style0">- אה, אה,
                אה,
            </ns0:p>
            <ns0:p xml:id="subtitle145" begin="2046215003t" end="2059982507t" region="region62" style="style0">עכשיו.
            </ns0:p>
            <ns0:p xml:id="subtitle146" begin="2060400007t" end="2081672508t" region="region6" style="style0">זה בסדר,
            </ns0:p>
            <ns0:p xml:id="subtitle147" begin="2060400007t" end="2081672508t" region="region29" style="style0">אתה לא
                צריך
            </ns0:p>
            <ns0:p xml:id="subtitle148" begin="2082079999t" end="2099187506t" region="region21" style="style0">להתנצל.
            </ns0:p>
            <ns0:p xml:id="subtitle149" begin="2099605006t" end="2112945000t" region="region63" style="style0">
                תפספסתי.
            </ns0:p>
            <ns0:p xml:id="subtitle150" begin="2113362500t" end="2130052507t" region="region51" style="style0">תביעת
                פטנט.
            </ns0:p>
            <ns0:p xml:id="subtitle151" begin="2113362500t" end="2130052507t" region="region2" style="style0">ניהלתי
                עסקה
            </ns0:p>
            <ns0:p xml:id="subtitle152" begin="2130470007t" end="2142557500t" region="region34" style="style0">כדי לסיים
                את זה.
            </ns0:p>
            <ns0:p xml:id="subtitle153" begin="2142975000t" end="2162995000t" region="region64" style="style0">- מה
                אתה
            </ns0:p>
            <ns0:p xml:id="subtitle154" begin="2142975000t" end="2162995000t" region="region65" style="style0">מדבר
                עליו?
            </ns0:p>
            <ns0:p xml:id="subtitle155" begin="2163412500t" end="2188025005t" region="region66" style="style0">הסכמתי
                להגיש אותו בשבילי.
            </ns0:p>
            <ns0:p xml:id="subtitle156" begin="2163412500t" end="2188025005t" region="region29" style="style0">וכל שעלי
                לעשות זה
            </ns0:p>
            <ns0:p xml:id="subtitle157" begin="2188442506t" end="2216802503t" region="region51" style="style0">לבדוק את
                המסמכים שלו.
            </ns0:p>
            <ns0:p xml:id="subtitle158" begin="2188442506t" end="2216802503t" region="region67" style="style0">- אה.
                זה
            </ns0:p>
            <ns0:p xml:id="subtitle159" begin="2238075005t" end="2256007502t" region="region68" style="style0">מסביר
                למה
            </ns0:p>
            <ns0:p xml:id="subtitle160" begin="2271027508t" end="2285202502t" region="region13" style="style0">הוא אמר
                לתת לך את זה...
            </ns0:p>
            <ns0:p xml:id="subtitle161" begin="2307727505t" end="2351107508t" region="region61" style="style0">ולמה
                הוא
            </ns0:p>
            <ns0:p xml:id="subtitle162" begin="2307727505t" end="2351107508t" region="region44" style="style0">קרא לך
                טמטום.
            </ns0:p>
            <ns0:p xml:id="subtitle163" begin="2351525008t" end="2385302502t" region="region12" style="style0">- מי
                מסדר?
            </ns0:p>
            <ns0:p xml:id="subtitle164" begin="2351525008t" end="2385302502t" region="region26" style="style0">- כולם.
            </ns0:p>
            <ns0:p xml:id="subtitle165" begin="2385720002t" end="2399905006t" region="region8" style="style0">- רגע
            </ns0:p>
            <ns0:p xml:id="subtitle166" begin="2385720002t" end="2399905006t" region="region69" style="style0">האם אלה
            </ns0:p>
            <ns0:p xml:id="subtitle167" begin="2400322507t" end="2419090006t" region="region70" style="style0">- עדיין
            </ns0:p>
            <ns0:p xml:id="subtitle168" begin="2400322507t" end="2419090006t" region="region71" style="style0">מדפיס.
            </ns0:p>
            <ns0:p xml:id="subtitle169" begin="2419507506t" end="2458295005t" region="region18" style="style0">- אה...
                זה
            </ns0:p>
            <ns0:p xml:id="subtitle170" begin="2419507506t" end="2458295005t" region="region27" style="style0">שישה
                מדפסות, 23 עמודים לדקה, 30 דקות.
            </ns0:p>
            <ns0:p xml:id="subtitle171" begin="2458712506t" end="2495830002t" region="region51" style="style0">זה
                4,140
            </ns0:p>
            <ns0:p xml:id="subtitle172" begin="2458712506t" end="2495830002t" region="region17" style="style0">עמודים.
            </ns0:p>
            <ns0:p xml:id="subtitle173" begin="2496247502t" end="2516685003t" region="region41" style="style0">- פלוס
                כל
            </ns0:p>
            <ns0:p xml:id="subtitle174" begin="2496247502t" end="2516685003t" region="region32" style="style0">זה.
            </ns0:p>
            <ns0:p xml:id="subtitle175" begin="2517102503t" end="2535035001t" region="region3" style="style0">מה שאומר
            </ns0:p>
            <ns0:p xml:id="subtitle176" begin="2517102503t" end="2535035001t" region="region25" style="style0">שבפעם
                הבאה
            </ns0:p>
            <ns0:p xml:id="subtitle177" begin="2535452502t" end="2563395000t" region="region16" style="style0">שאתה
                מנהל
            </ns0:p>
            <ns0:p xml:id="subtitle178" begin="2535452502t" end="2563395000t" region="region26" style="style0">- אני
            </ns0:p>
            <ns0:p xml:id="subtitle179" begin="2563812500t" end="2586755003t" region="region5" style="style0">- ממ.
            </ns0:p>
            <ns0:p xml:id="subtitle180" begin="2587172503t" end="2608862506t" region="region32" style="style0">מתי זה
            </ns0:p>
            <ns0:p xml:id="subtitle181" begin="2609280006t" end="2627212503t" region="region72" style="style0">(הרובוט
                של אימה
                <ns0:span style="style1">Greenback Boogie)</ns0:span>
            </ns0:p>
            <ns0:p xml:id="subtitle182" begin="2627630003t" end="2644310001t" region="region73" style="style0">-
                רואים את הכסף רוצה להישאר
            </ns0:p>
            <ns0:p xml:id="subtitle183" begin="2627630003t" end="2644310001t" region="region74" style="style0">לארוחת
                הערב שלך
            </ns0:p>
            <ns0:p xml:id="subtitle184" begin="2644727501t" end="2679350006t" region="region75" style="style0">קבל עוד
            </ns0:p>
            <ns0:p xml:id="subtitle185" begin="2644727501t" end="2679350006t" region="region74" style="style0">חתיכה של
                עוגה
            </ns0:p>
            <ns0:p xml:id="subtitle186" begin="2679767506t" end="2715215001t" region="region76" style="style0">לאישה שלך
            </ns0:p>
            <ns0:p xml:id="subtitle187" begin="2679767506t" end="2715215001t" region="region77" style="style0">כולם
                רוצים לדעת
            </ns0:p>
            <ns0:p xml:id="subtitle188" begin="2715632502t" end="2746497502t" region="region78" style="style0">איך זה
                מרגיש
            </ns0:p>
            <ns0:p xml:id="subtitle189" begin="2715632502t" end="2746497502t" region="region79" style="style0">כולם
                רוצים לראות
            </ns0:p>
            <ns0:p xml:id="subtitle190" begin="2746915003t" end="2777362503t" region="region78" style="style0">מה זה כמו
            </ns0:p>
            <ns0:p xml:id="subtitle191" begin="2746915003t" end="2777362503t" region="region77" style="style0">לחיות
                בקופסה דבורים
            </ns0:p>
            <ns0:p xml:id="subtitle192" begin="2777780003t" end="2819072506t" region="region80" style="style0">אני לא
                מתנגד
            </ns0:p>
            <ns0:p xml:id="subtitle193" begin="2777780003t" end="2819072506t" region="region81" style="style0">להיות
                עסוק
            </ns0:p>
            <ns0:p xml:id="subtitle194" begin="2819490006t" end="2832005008t" region="region82" style="style0">כל זה
            </ns0:p>
            <ns0:p xml:id="subtitle195" begin="2832422508t" end="2847432503t" region="region83" style="style0">כן, היי,
            </ns0:p>
            <ns0:p xml:id="subtitle196" begin="2847850003t" end="2887055003t" region="region78" style="style0">אני צריך
                לרקוד
            </ns0:p>
            <ns0:p xml:id="subtitle197" begin="2887472503t" end="2907075003t" region="region84" style="style0">הבוגי של
                הכסף
            </ns0:p>
            <ns0:p xml:id="subtitle198" begin="2960465006t" end="2977145003t" region="region82" style="style0">- נחמד
                מצידך
            </ns0:p>
            <ns0:p xml:id="subtitle199" begin="2960465006t" end="2977145003t" region="region85" style="style0">להצטרף
                אלינו\n הבוקר.\n
            </ns0:p>
            <ns0:p xml:id="subtitle200" begin="2977562503t" end="2995077501t" region="region86" style="style0">- למידע
                שלך,
            </ns0:p>
            <ns0:p xml:id="subtitle201" begin="2977562503t" end="2995077501t" region="region87" style="style0">מר.
                סאנשיין,
            </ns0:p>
            <ns0:p xml:id="subtitle202" begin="2995495001t" end="3008010003t" region="region88" style="style0">אני כאן
                כל הלילה
            </ns0:p>
            <ns0:p xml:id="subtitle203" begin="2995495001t" end="3008010003t" region="region89" style="style0">עובר על
                הטקסט.
            </ns0:p>
            <ns0:p xml:id="subtitle204" begin="3008427505t" end="3023020000t" region="region90" style="style0">ששייך
                ל--
            </ns0:p>
            <ns0:p xml:id="subtitle205" begin="3008427505t" end="3023020000t" region="region91" style="style0">OKAY.
            </ns0:p>
            <ns0:p xml:id="subtitle206" begin="3023437500t" end="3039710006t" region="region75" style="style0">- איפה
                האישור שלי?
            </ns0:p>
            <ns0:p xml:id="subtitle207" begin="3040127506t" end="3055137501t" region="region92" style="style0">- WHAT?
            </ns0:p>
            <ns0:p xml:id="subtitle208" begin="3055555001t" end="3075992502t" region="region93" style="style0">- העובדה
                שאתה לא יודע
            </ns0:p>
            <ns0:p xml:id="subtitle209" begin="3055555001t" end="3075992502t" region="region94" style="style0">כבר
                מדאיגה אותי.
            </ns0:p>
            <ns0:p xml:id="subtitle210" begin="3076410002t" end="3092265008t" region="region95" style="style0">- אה, אתה
                מתכוון
            </ns0:p>
            <ns0:p xml:id="subtitle211" begin="3076410002t" end="3092265008t" region="region96" style="style0">לדבר
                הפטנט?
            </ns0:p>
            <ns0:p xml:id="subtitle212" begin="3092682508t" end="3118537505t" region="region97" style="style0">- "הדבר
                הפטנטי"
            </ns0:p>
            <ns0:p xml:id="subtitle213" begin="3092682508t" end="3118537505t" region="region98" style="style0">היא
                היחידה
                של נייר
            </ns0:p>
            <ns0:p xml:id="subtitle214" begin="3118955005t" end="3145227501t" region="region99" style="style0">שמעכב
                את העסקה שלנו
            </ns0:p>
            <ns0:p xml:id="subtitle215" begin="3118955005t" end="3145227501t" region="region85" style="style0">מאתמול.
            </ns0:p>
            <ns0:p xml:id="subtitle216" begin="3145645001t" end="3156490002t" region="region80" style="style0">איפה הוא
                נמצא?
            </ns0:p>
            <ns0:p xml:id="subtitle217" begin="3156907502t" end="3173170000t" region="region100" style="style0">- על
                השולחן שלי.
            </ns0:p>
            <ns0:p xml:id="subtitle218" begin="3173587500t" end="3191530007t" region="region101" style="style0">- ובכן,
                אם
                הוא לא על שלי
            </ns0:p>
            <ns0:p xml:id="subtitle219" begin="3173587500t" end="3191530007t" region="region102" style="style0">עד
                שאחזור,
                אני אפתיח אותך ללואיס.
            </ns0:p>
            <ns0:p xml:id="subtitle220" begin="3191947508t" end="3211132507t" region="region73" style="style0">- אין
